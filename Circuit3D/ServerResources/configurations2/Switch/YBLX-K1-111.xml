<?xml version="1.0" encoding="UTF-8"?>
<!-- 行程开关 (三种位置关系) -->
<ElecCompDef name="行程开关" model="YBLX-K1/111" desc="行程开关在在电路中用SQ表示">
	<Terminal id="1" name="NC_1" mdlName="1" />
	<Terminal id="2" name="NC_2" mdlName="2" />
	<Terminal id="3" name="NO_1" mdlName="3" />
	<Terminal id="4" name="NO_2" mdlName="4" />

	<!-- 开关的两端 -->
	<!-- momentary表示是否回弹 -->
	<!-- position:默认位置 0=closed, 1=open -->
	<CircuitElm type="SwitchElm" id="s1" term1="NC_1" term2="NC_2" momentary="false" position="0"/>
	<CircuitElm type="SwitchElm" id="s2" term1="NO_1" term2="NO_2" momentary="false" position="1"/>

	<!-- interact：按钮的交互方式 -->
	<!-- effect：表示按钮可以操作哪些“内部”开关SwitchElm -->
	<ControlIO interact="press" name="按钮" mdlName="NP4-11BN_05" effect="s1,s2" >

	</ControlIO>

	<!-- 额外的配置信息 -->
	<Params>
		<Param key="shell" value="YBLX-K1_03,YBLX-K1_04,YBLX-K1_15,YBLX-K1_01" />
	</Params>
</ElecCompDef>