<ElecCompDef name="接触器辅助触点" model="F4-22" desc="F4-22辅助触点可以和CJX2系列接触器配合使用，解决控制触点数量不足的问题。">
	<RelyOn type="2" localTranslation="0,0,0" localRotation="0,0,0" relyIds="off,on"/>

	<Terminal id="53NO" name="53NO" mdlName="53NO" direction="Z-" />
	<Terminal id="61NC" name="61NC" mdlName="61NC" direction="Z-" />
	<Terminal id="71NC" name="71NC" mdlName="71NC" direction="Z-" />
	<Terminal id="83NO" name="83NO" mdlName="83NO" direction="Z-" />
	<Terminal id="54NO" name="54NO" mdlName="54NO" direction="Z+" />
	<Terminal id="62NC" name="62NC" mdlName="62NC" direction="Z+" />
	<Terminal id="72NC" name="72NC" mdlName="72NC" direction="Z+" />
	<Terminal id="84NO" name="84NO" mdlName="84NO" direction="Z+" />
	<Magnetism>
		<ControlIO type="input,output" interact="press" name="触点架" mdlName="F4-22_32" switchIn="off,on" motion="move" motionParams="0,-0.005" axis="y">
			<Param key="sound" value="me/Interface/sound/control/press.wav"/>
		</ControlIO>
	</Magnetism>
	<ResisState id="off" isDef="1">
		<ResisRelation term1Id="61NC" term2Id="62NC" value="0" />
		<ResisRelation term1Id="71NC" term2Id="72NC" value="0" />
	</ResisState>
	<ResisState id="on">
		<ResisRelation term1Id="53NO" term2Id="54NO" value="0" />
		<ResisRelation term1Id="83NO" term2Id="84NO" value="0" />
	</ResisState>
	<Params>
		<Param key="shell" value="F4-22_00,F4-22_01" />
	</Params>
</ElecCompDef>